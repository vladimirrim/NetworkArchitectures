<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ru.spbau.egorov.net_arch.controller.ServerMenuController">
    <Button mnemonicParsing="false" onAction="#runNonblocking" prefHeight="200.0" prefWidth="200.0"
            text="Run Nonblocking" VBox.vgrow="ALWAYS"/>
    <Button mnemonicParsing="false" onAction="#runMultiThread" prefHeight="200.0" prefWidth="200.0"
            text="Run MultiThread" VBox.vgrow="ALWAYS"/>
    <Button mnemonicParsing="false" onAction="#rumMultiThreadWithWriteThread" prefHeight="200.0" prefWidth="200.0"
            text="Run MultiThreadWithWrite" VBox.vgrow="ALWAYS"/>
    <TextArea fx:id="hostName" prefHeight="200.0" prefWidth="200.0" promptText="Enter server hostname"/>
    <TextArea fx:id="port" prefHeight="200.0" prefWidth="200.0" promptText="Enter port"/>
</VBox>
