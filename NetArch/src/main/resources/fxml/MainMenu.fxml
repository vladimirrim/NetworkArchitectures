<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<SplitPane dividerPositions="0.5" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
           prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.spbau.egorov.net_arch.controller.MainMenuController">
    <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
        <Button fx:id="setupClientButton" onAction="#setupClient" mnemonicParsing="false" prefHeight="404.0"
                prefWidth="295.0" text="Setup client"
                VBox.vgrow="ALWAYS">
            <font>
                <Font name="Times New Roman Bold" size="39.0"/>
            </font>
        </Button>
    </VBox>
    <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
        <Button fx:id="setupServerButton" onAction="#setupServer" mnemonicParsing="false" prefHeight="404.0" prefWidth="295.0" text="Setup server"
                VBox.vgrow="ALWAYS">
            <font>
                <Font name="Times New Roman Bold" size="39.0"/>
            </font>
        </Button>
    </VBox>
</SplitPane>
